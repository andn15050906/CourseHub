              $.get(url, function(data){
                  if(data != ''){
                      $('.list-course').append(data.str_return);
                      checkShowButtonMore();
                  }
              },'json');
          }
      
          function checkShowButtonMore(){
              var flag = true;
              var countCourse = $(".list-course").children().length;
